{
  "editor.inlayHints.enabled": "offUnlessPressed",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "rust-analyzer.workspace.discoverConfig": null,
  "editor.formatOnSave": true,
  "git.confirmSync": false,
  "workbench.colorTheme": "One Dark Pro Darker",
  "breadcrumbs.enabled": false,
  "rust-analyzer.lens.implementations.enable": false,
  "editor.stickyScroll.enabled": false,
  "editor.semanticTokenColorCustomizations": {},
  "vscode-neovim.neovimClean": true,
  "editor.codeLens": false,
  "rust-analyzer.check.command": "clippy",
  "workbench.editor.customLabels.patterns": {
    "**/mod.rs": "${dirname}/mod.rs",
    "**/page.tsx": "${dirname}/page.tsx",
    "**/layout.tsx": "${dirname}/layout.tsx"
  },
  "editor.fontLigatures": false,
  "editor.fontFamily": "DejaVuSansM Nerd Font Mono",
  "window.commandCenter": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "cursor-retrieval.canAttemptGithubLogin": false,
  "editor.lineNumbers": "relative",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
